#include "main.h"
#include <stdio.h>
/**
 * add - check of a number
 * @a: the number to check
 * @b: the  untg
 * Return: last correct
 */
void print_to_98(int n)
{
for (n = 0; n <= 98; n++)
{
if (n < 9)
{
_putchar(n + '0');
_putchar(',');
_putchar(' ');
}
else
_putchar(n % 10);
}
_putchar('\n');
}
